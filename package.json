    "babel-loader": "^9.1.3",
    "discord-api-types": "^0.37.90",
    "dotenv": "^16.4.5",
    "electron-is-dev": "^2.0.0",
    "express": "^4.19.2",
    "install": "^0.13.0",
    "keytar": "^7.9.0",
    "node-fetch": "^3.3.2",
    "node-vibrant": "^3.2.1-alpha.1",
    "npm": "^10.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "react-starfield": "^1.0.3",
    "sass": "^1.77.5",
